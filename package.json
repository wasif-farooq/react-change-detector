{
  "name": "react-change-detector",
  "version": "0.1.5",
  "description": "This package enable you to use change detector in you react form or app only on the states you provide to it",
  "main": "lib/index.js",
  "files": [
    "/lib"
  ],
  "scripts": {
    "start": "webpack-dev-server --config ./webpack/webpack.dev.config.js --mode development",
    "build": "webpack --config ./webpack/webpack.config.js --mode production",
    "transpile": "./node_modules/@babel/cli/bin/babel.js src -d lib --copy-files",
    "deploy": "gh-pages -d dist",
    "predeploy": "npm run build"
  },
  "peerDependencies": {
    "react": "^16.6.1",
    "react-dom": "^16.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "css-loader": "^1.0.1",
    "file-loader": "^2.0.0",
    "gh-pages": "^2.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.5.0",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-hot-loader": "^4.8.2",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "keywords": [
    "form",
    "react",
    "change detector"
  ],
  "author": "Wasif Farooq",
